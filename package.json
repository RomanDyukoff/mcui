{
  "name": "mediacube-ui",
  "version": "0.1.271",
  "description": "Design system for Mediacube services",
  "author": "Mediacube",
  "private": false,
  "homepage": "https://mediacube.co",
  "license": "MIT",
  "main": "dist/mediacube-ui.common.js",
  "files": [
    "src/assets/fonts",
    "src/assets/img",
    "src/assets/tokens",
    "src/assets/icons.svg",
    "src/helpers",
    "src/styles",
    "src/tokens",
    "src/utils",
    "src/mixins",
