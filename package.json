  "scripts": {
    "start": "yarn scss-export && start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "serve": "yarn scss-export && vue-cli-service serve",
    "build": "yarn scss-export && vue-cli-service build --target lib --inline-vue",
