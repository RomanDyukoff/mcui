{
  "name": "mcui",
  "version": "0.1.0",
  "description": "Design system for MediaCube services",
  "author": "MediaCube",
  "private": false,
  "homepage": "https://mediacube.co",
  "license": "MIT",
  "main": "dist/mcui.common.js",
  "scripts": {
    "start": "yarn scss-export && start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "serve": "yarn scss-export && vue-cli-service serve",
    "build": "yarn scss-export && vue-cli-service build --target lib --inline-vue",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "scss-export": "sass-export src/tokens/*.scss -o src/assets/tokens/tokens.json",
    "release": "yarn build && standard-version",
    "release:patch": "yarn release -- --release-as patch",
    "release:minor": "yarn release -- --release-as minor",
    "release:major": "yarn release -- --release-as major",
    "publish:patch": "yarn release:patch && yarn publish . && git push",
    "publish:minor": "yarn release:minor && yarn publish . && git push",
    "publish:major": "yarn release:major && yarn publish . && git push"
  },
  "dependencies": {
    "bourbon": "^7.0.0",
    "core-js": "^3.6.5",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/vue": "^5.3.19",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "jest-vue-preprocessor": "^1.7.1",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "sass-export": "^1.0.6",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.0.3",
    "standard-version": "^8.0.0",
    "storybook-addon-designs": "^5.4.0",
    "storybook-design-token": "^0.7.7",
    "style-loader": "^1.2.1",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11"
  }
}
